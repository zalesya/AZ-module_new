<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="userService" class="by.nc.school.dev.service.impl.UserServiceImpl">
        <property name="userRepository" ref="userRepository" />
    </bean>

    <bean id="studentService" class="by.nc.school.dev.service.impl.StudentServiceImpl">
        <property name="studentRepository" ref="studentRepository" />
    </bean>

    <bean id="professorService" class="by.nc.school.dev.service.impl.ProfessorServiceImpl">
        <property name="professorRepository" ref="professorRepository" />
    </bean>

</beans>